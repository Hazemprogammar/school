{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./",
    "types": ["node"],
    "strict": false,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "skipDefaultLibCheck": false,
    "paths": {
      "@/*":["src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]


}
